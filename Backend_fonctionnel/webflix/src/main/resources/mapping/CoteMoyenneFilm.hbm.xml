<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pojo.CoteMoyenneFilm" table="COTE_MOYENNE_FILM">
        <subselect>select * from cote_moyenne_film</subselect>
        
        <id name="idFilm" type="int" column="IDFILM"/>
        <property name="coteAvg" column="COTE_AVG" type="double"/>

    </class>
</hibernate-mapping>
