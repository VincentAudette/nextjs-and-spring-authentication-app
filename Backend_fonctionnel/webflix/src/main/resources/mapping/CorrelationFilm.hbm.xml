<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pojo.CorrelationFilm" table="CORRELATION_FILM">
<!--        <subselect>select * from CORRELATION_FILM</subselect>-->
        <composite-id>
            <key-property name="idFilmJ" type="int" column="IDFILM_J"/>
            <key-property name="idFilmK" type="int" column="IDFILM_K"/>
        </composite-id>
        <property name="correlation" column="CORRELATION" type="double"/>
    </class>
</hibernate-mapping>
