<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pojo.OlapClient" table="OLAP_CLIENT">
        <id name="idClient" type="int" column="ID_CLIENT"></id>
        <property name="nom" column="NOM" type="string"/>
        <property name="groupeAge" column="GROUPE_AGE" type="int"/>
        <property name="anciennete" column="ANCIENNETE" type="date"/>
        <property name="codePostal" column="CODE_POSTAL" type="string"/>
        <property name="ville" column="VILLE" type="string"/>
        <property name="province" column="PROVINCE" type="string"/>
    </class>
</hibernate-mapping>