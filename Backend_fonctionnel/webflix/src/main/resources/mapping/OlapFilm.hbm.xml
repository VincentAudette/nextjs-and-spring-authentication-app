<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pojo.OlapFilm" table="OLAP_FILM">
        <id name="idFilm" type="int" column="ID_FILM"></id>
        <property name="titre" column="TITRE" type="string"/>
        <property name="anneeSortie" column="ANNEE_SORTIE" type="int"/> 
        <property name="pays" column="PAYS" type="int"/>
        <property name="action" column="ACTION_G" type="int"/>
        <property name="adventure" column="ADVENTURE_G" type="int"/>
        <property name="comedy" column="COMEDY_G" type="int"/>
        <property name="family" column="FAMILY_G" type="int"/>
        <property name="romance" column="ROMANCE_G" type="int"/>
        <property name="drama" column="DRAMA_G" type="int"/>
        <property name="animation" column="ANIMATION_G" type="int"/>
        <property name="fantasy" column="FANTASY_G" type="int"/>
        <property name="biography" column="BIOGRAPHY_G" type="int"/>
        <property name="thriller" column="THRILLER_G" type="int"/>
        <property name="sciFi" column="SCI_FI_G" type="int"/>
        <property name="crime" column="CRIME_G" type="int"/>
        <property name="sport" column="SPORT_G" type="int"/>
        <property name="horror" column="HORROR_G" type="int"/>
        <property name="filmNoir" column="FILM_NOIR_G" type="int"/>
        <property name="mystery" column="MYSTERY_G" type="int"/>
        <property name="western" column="WESTERN_G" type="int"/>
        <property name="war" column="WAR_G" type="int"/>
        <property name="musical" column="MUSICAL_G" type="int"/>
        <property name="documentary" column="DOCUMENTARY_G" type="int"/>
        <property name="history" column="HISTORY_G" type="int"/>
        <property name="music" column="MUSIC_G" type="int"/>
    </class>
</hibernate-mapping>