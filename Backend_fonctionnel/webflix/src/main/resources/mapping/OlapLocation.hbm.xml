<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pojo.OlapLocation" table="OLAP_LOCATION">
        <id name="idLocation" type="int" column="ID_LOCATION"></id>
        <property name="cote" column="COTE" type="int"/>
        <property name="groupeAgeClient" column="GROUPE_AGE_CLIENT" type="int"/>
        <property name="provinceClient" column="PROVINCE_CLIENT" type="string"/>
        <property name="jourDeSemaine" column="JOUR_DE_SEMAINE" type="int"/>
        <property name="mois" column="MOIS" type="int"/>

        <many-to-one insert="false" update="false" lazy="false" fetch="select" name="olapFilm" column="ID_FILM" />
        <many-to-one insert="false" update="false" lazy="false" fetch="select" name="olapClient" column="ID_CLIENT"/>
        <many-to-one insert="false" update="false" lazy="false" fetch="select" name="olapTemps" column="ID_TEMPS"/>
    </class>
</hibernate-mapping>